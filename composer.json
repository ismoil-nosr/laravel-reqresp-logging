{
    "name": "ismoil-nosr/laravel-reqresp-logging",
    "description": "Log requests and responses in your laravel project",
    "keywords": [
        "ismoil-nosr",
        "laravel-reqresp-logging"
    ],
    "homepage": "https://github.com/ismoil-nosr/laravel-reqresp-logging",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ismoil Nosr",
            "email": "ismoil.nosr@gmail.com",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "IsmoilNosr\\LaravelReqrespLogging\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IsmoilNosr\\LaravelReqrespLogging\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "IsmoilNosr\\LaravelReqrespLogging\\LaravelReqrespLoggingServiceProvider"
            ],
            "aliases": {
                "LaravelReqrespLogging": "IsmoilNosr\\LaravelReqrespLogging\\LaravelReqrespLoggingFacade"
            }
        }
    }
}
